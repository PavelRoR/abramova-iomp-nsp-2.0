*
	margin 0
	padding 0
	outline none
body
	font-size: 19px
	font-family: SUD
	font-weight: 300
	color #000
	background-color #fff
	line-height: 26px
.container
	width 1200px
	// padding 0
	margin: 0 auto;
	position relative
	@media screen and (min-width 2295px)
		max-width 1400px
	// @media screen and (min-width 1200px)
	// 	width: 1170px;
	// @media screen and (min-width 992px)
	// 	width: 970px;
	// @media screen and (min-width 768px)
	// 	width: 750px;
.content
	position relative
	z-index 50
.wrapper
	overflow hidden
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight 500
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	text-align center
	margin 0 auto
	text-decoration none
	color: #FEE7E6
	width: auto
	font-family: L
	background-color: #48817B;
	border-radius: 5px;
	padding 17px 74px
	transition: all .3s ease
	&:focus
		background-color: #48817B;
		text-decoration none
		color #FEE7E6
	&:hover
		background-color: #2B514D
		text-decoration none
		color #FEE7E6
.general-title
	font-family: L
	font-size: 38px
	margin-bottom: 70px
	&-pink
		color: #FEE7E6;
.pre-title
	font-size: 24px
	font-weight: 300
	margin-bottom: 20px
	color: #ADADAD;
.general-text
	margin-bottom: 30px
	@media screen and (max-width 992px)
		margin-bottom: 20px
	@media screen and (max-width 560px)
		margin-bottom: 10px
.general-list-ul
	li
		list-style: none
		position relative
		padding-left: 38px
		margin-bottom: 60px
		&:before
			content: ''
			display block
			position absolute
			width 18px
			height: 18px
			border-radius: 50px
			background-color #48817B
			top 5px
			left 0
	&-pink
		li
			color: #FEE7E6
			padding-left: 28px
			margin-bottom: 30px
			&:before
				width 13px
				height: 13px
				top 7px
				background-color: #FEE7E6
.general-list-ol
	position relative
	color: #FEE7E6
	margin-bottom: 70px
	width 100%
	&-title
		display block
		font-family: L
		font-size: 38px
		line-height: 45px
		position absolute
		top 0
		left 0
	&-text
		font-family: SUD
		font-size: 19px
		line-height: 27px
		padding-left: 40px